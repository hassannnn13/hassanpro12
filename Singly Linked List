//Node at Beg
class Node {
    int data;
    Node next;

    Node(int data) {
        this.data = data;
        this.next = null;
    }
}

class LinkedList {
    Node head;

    public void addAtBeg(int data) {
        Node newNode = new Node(data);
         newNode.next = head;
         head = newNode;
}
 public void printList() {
        Node currentNode = head;
        while (current != null) {
            System.out.print(currentNode.data + " -> ");
            currentNode = currentNode.next;
        }
        System.out.println("None");
    }
}

public class LinkedList {
    public static void main(String[] args) {
        LinkedList linkedList = new LinkedList();
        linkedList.addAtBeg(10);
    

        linkedList.printList();
    }
}

//Node At last

class Node {
    int data;
    Node next;

    Node(int data) {
        this.data = data;
        this.next = null;
    }
}

class LinkedList {
    Node head;

    public void addAtLast(int data) {
        if(head==null){
head = newNode;
}
else{
Node cuurent = head;
while(current !=null){
current = current.next;
}
current.next = newNode;
}
}
 public void printList() {
        Node currentNode = head;
        while (currentNode != null) {
            System.out.print(currentNode.data + " -> ");
            currentNode = currentNode.next;
        }
        System.out.println("None");
    }
}

public class LinkedList {
    public static void main(String[] args) {
        LinkedList linkedList = new LinkedList();
        linkedList.addAtLast(10);
    

        linkedList.printList();
    }
}


// Delete Node at First
Node DeleteFirst(){
if (head ! = null){
Node toDelete = head;
head= head.next;
return toDelete;
}
return null;
}

//Delete Node at Last
Node DeleteLast(){
Node current = head;
if (head == null || head .next == null){
head = null;
return current;
}
Node nextNode = current.next;
while(current.next != null){
if (nextNode ==  null){
current.next = null;
}
current = nextNode;
nextNode = nextNode.next;
}
return current;
}


